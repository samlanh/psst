					+'<td style="white-space:nowrap; border-right: 0px none;" colspan="1"><?php echo $tr->translate('ALL');?> <br><input style="height: 13px;" type="checkBox" value="0" checked="checked" name="checkall" onclick="check('+respone.length+');setValue('+respone.length+');" id="checkall"/></td>'
				    +'<td><?php echo $tr->translate('NUM');?></td>'
					+'<td><?php echo $tr->translate('STUDENT_CODE');?></td>'
					+'<td><?php echo $tr->translate('STUDENT_NAMEKHMER');?></td>'
					+'<td><?php echo $tr->translate('NAME_EN');?></td>'
					+'<td ><?php echo $tr->translate('SEX');?></td>'
					+'<td ><?php echo $tr->translate('GROUP');?></td>'
					+'<td ><?php echo $tr->translate('ACADEMIC_YEAR_ENROLL');?></td>'
					+'<td ><?php echo $tr->translate('STUDENT_TYPE');?></td>'
					+'</tr>'
				+'</thead>';
			if(respone!=""){
				num=0;
				all_student = respone.length;
				for(var i=0;i<respone.length;i++){
					inx = i+1;
					tem += '<tr style="border:1px solid #000;">';
					tem += '<td align="center"><input style="height: 13px;" class="print-checkbox"  type="checkBox" checked="checked" value="'+inx+'" onclick="setValue('+respone.length+');" name="checkbox'+inx+'" id="checkbox'+inx+'"/></td>';
					tem += '<td align="center">'+inx+' ';
					tem += '<input type="hidden"  name="stu_id_'+inx+'" id="stu_id_'+inx+'" value="'+respone[i].stu_id+'" />';
					tem += '<input type="hidden"  name="degree_'+inx+'" id="degree_'+inx+'" value="'+respone[i].degree+'" />';
					tem += '<input type="hidden"  name="grade_'+inx+'" id="grade_'+inx+'" value="'+respone[i].grade+'" />';
					tem += '</td>';
					tem += '<td>&nbsp;'+respone[i].stu_code+'</td>';
					tem += '<td>&nbsp;'+respone[i].stu_khname+'</label></td>';
					tem += '<td>&nbsp;'+respone[i].last_name+" "+respone[i].stu_enname+'</td>';
					tem += '<td align="center">&nbsp;'+respone[i].sex+'</td>';
					tem += '<td align="center">'+respone[i].grade_title+'- '+respone[i].groupCode+'</td>';
					tem += '<td align="center">'+respone[i].startYear+'</td>';
					tem += '<td align="center">'+respone[i].studentType+'</td>';
					tem += '</tr>';
					if($("#identity").val()!="") {
						var identity = $("#identity").val();
						$("#identity").val(identity+','+inx);
					} else {$("#identity").val(inx);}
				}
			}
			tem+='</table>';
			dojo.byId('data_table').innerHTML = tem;
			HideloadingBlock();
		},
		error: function(err) {
			HideloadingBlock();
		}
	});
}


function setValue(i){
	$('#identity').val('');
	for(var j=1;j<=i;j++){
		if(dojo.byId('checkbox'+j).checked){
			if($("#identity").val()!="") {
				var iden = $("#identity").val();
				$("#identity").val(iden+','+j);
			} else{
				$("#identity").val(j);
			}
		}
	}
	
}

function check(i){
	if($('#checkall').is(":checked")){setValue
		for(var j=1;j<=i;j++){
			$('#checkbox'+j).each(function() { //loop through each checkbox
	            this.checked = true;  
			});
		}
	}else{
		for(var j=1;j<=i;j++){
			$("#checkbox"+j).removeAttr("checked");
		}
	}
}

function doPrintRowCheck() {
	var table = document.getElementById('divPrint');
    var checkboxes = table.getElementsByClassName('print-checkbox');
    var checkedRows = [];
    var rowNum = 1; // Initialize row number

    // Loop through all checkboxes
    for (var i = 0; i < checkboxes.length; i++) {
        var checkbox = checkboxes[i];
        var row = checkbox.closest('tr'); // Get the parent row
        var cells = row.cells;
        
        // If checkbox is checked, add row HTML to checkedRows array
        if (checkbox.checked) {
            var rowHtml = '<tr>';
            
            // Adjust the number column to be reset
            rowHtml += '<td class="center">' + rowNum + '</td>';
            rowNum++; // Increment row number
            
            for (var j = 1; j < cells.length; j++) { // Start from 1 to skip the first hidden column
                var hideclass = '';
                if (j == 1) {
                    hideclass = 'hidden';
                }
                rowHtml += '<td class="' + hideclass + '">' + cells[j].textContent + '</td>';
            }
            rowHtml += '</tr>';
            checkedRows.push(rowHtml);
        }
    }

////Print 

  var w = 1000;
  var h = 600;
