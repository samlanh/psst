url_getacademic = '<?php echo $this->url(array('module'=>'accounting','controller'=>'fee','action'=>'getfeeid'));?>';
function getAllAcademicByBranch(){
	
	dijit.byId('academic_year_fee').reset();
	branch_id = dijit.byId('branch_id').get('value');
	if(branch_id=='' || branch_id==-1){
		var academic_store  = getDataStorefromJSON('id','name',<?php print_r(Zend_Json::encode(array()));?> );
		dijit.byId('academic_year_fee').set('store',academic_store);  
		dijit.byId('branch_id').focus();
		return false;
	}
	dojo.xhrPost({
		url: url_getacademic,
		content:{
			'branch_id':branch_id,
			'selectOption':1,
			'isFinished':0
			},
		handleAs:"json",
		load: function(data){
			academic_store  = getDataStorefromJSON('id','name', data);
			dijit.byId('academic_year_fee').set('store',academic_store);  
			dijit.byId('fromFeeid').set('store',academic_store);  
			if(oldFeeId>0){
				dijit.byId('fromFeeid').attr('value',oldFeeId);
			}
			dijit.byId('branch').set('value',branch_id);  
		},
		error: function(err) {
		}
	});
}

var grade_store  = getDataStorefromJSON('id','name', <?php print_r(array())?> );
var urlGetGrade = '<?php echo $this->url(array('module'=>'global','controller'=>'grade','action'=>'get-grade')); ?>';
function getallGrade(){
	dijit.byId('grade').attr('value','');   
	dept_id = dijit.byId('degree').get('value');
		if(dept_id==''){
			return false;
		}
		dojo.xhrPost({
			url:urlGetGrade,
			content:{
				'dept_id':dept_id,
				'noaddnew':1
				},
			handleAs:"json",
			load: function(data) {
				grade_store  = getDataStorefromJSON('id','name', data);
			    dijit.byId('grade').set('store',grade_store);  
				if(oldGrade>0){
					dijit.byId('grade').attr('value',oldGrade);
				} 
			},
			error: function(err) {
			}
		});
}

var url_get_all_student_from_group = "<?php echo $this->url(array('module'=>'accounting','controller'=>'updatestudentfee','action'=>'get-all-student')); ?>";
function getStudentFromSearch(){
	loadingBlock()
	$("#identity").val('');
	
	fromFeeid = dijit.byId('fromFeeid').get('value');
	if(fromFeeid==0 || fromFeeid==""){
		alert('<?php echo $tr->translate('PLEASE_SELECT_STUDENT_FEE'); ?>');
		dijit.byId('fromFeeid').focus();
		HideloadingBlock();
		return false;
	}

	branch_id = dijit.byId('branch_id').get('value');
	academicYearEnroll = dijit.byId('academicYearEnroll').get('value');
