var url_getstuno = '<?php echo $this->url(array('module'=>'foundation','controller'=>'kindergarten','action'=>'get-stu-no')); ?>';
function getStudentNo(newid){
		//alert(dept_id);
		dojo.xhrPost({
			url:url_getstuno,
			content:{
				'newid':newid,
				},
			handleAs:"json",
			load: function(data) {
				//alert(data);
				//url_getstuno  = getDataStorefromJSON('id','name', data);
				dijit.byId('student_id').attr('value',data);   
			},
			error: function(err) {
				alert(err);
			}
		});
}

function hideDialogJob() {		
	dijit.byId("popup_job").hide();
}

var url_add_job = '<?php echo $this->url(array("module"=>"foundation","controller"=>"kindergarten","action"=>"add-job")); ?>';
function addJob(){
	
	type=dijit.byId('value_j').get('value');

	mo_job=dijit.byId('mo_job').get('value');
	fa_job=dijit.byId('fa_job').get('value');
	
	//alert(fa_job,mo_job);
	
	dojo.xhrPost({
		url:url_add_job,
		form: dojo.byId("frm_add_tran1"),
		handleAs:"json",
		load: function(data) {
			//alert(data); 
			var Itemmake = { 
			    		id: data,
				        name: dijit.byId('occu_name').get('value')
				   };
		if(type==1){	   
	 		addDataToSelectbox(dijit.byId('fa_job'), job_store, Itemmake, data);
			addDataToSelectbox(dijit.byId('mo_job'), job_store1, Itemmake, data);
			dijit.byId('mo_job').attr('value',mo_job);
			//$(this).unbind(event);
		}else if(type==2){
			addDataToSelectbox(dijit.byId('fa_job'), job_store, Itemmake, data);
			addDataToSelectbox(dijit.byId('mo_job'), job_store1, Itemmake, data);
			dijit.byId('fa_job').attr('value',fa_job);
			//$(this).unbind(event);
		}
		
	    dijit.byId('frm_add_tran1').reset();
			    
	    dijit.byId("popup_job").hide();
		},
		error: function(err) {
			alert(err);
		}
	});

}

