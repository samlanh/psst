//set hour defuale
function setHourDefault(){
	dept=dijit.byId('dept').get('value');
	session=dijit.byId('session').get('value');
	if(session==1 && (dept==1 || dept==3 || dept==4)){
		dijit.byId('study_hour').attr('value','7:30 - 10:30');
	}else if(session==2 && (dept==1 || dept==3)){
		dijit.byId('study_hour').attr('value','13:30 - 16:30');
	}else if(session==1 && dept==2){
		dijit.byId('study_hour').attr('value','7:30 - 11:30');
	}else if(session==2 && dept==2){
		dijit.byId('study_hour').attr('value','13:00 - 17:00');
	}
	else if(session==3 && dept==5){
		dijit.byId('study_hour').attr('value','17:30 - 19:00');
	}
	else if(session==4 && dept==6){
		dijit.byId('study_hour').attr('value','7:30 - 16:30');
	}else{
		dijit.byId('study_hour').attr('value','');
	}
}
