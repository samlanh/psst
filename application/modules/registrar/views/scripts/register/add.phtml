		//dijit.byId('dept').set('readOnly',false);
		//dijit.byId('grade').set('readOnly',false);
		//dijit.byId('session').set('readOnly',false);
		//dijit.byId('room').set('readOnly',false);
		return false;
	}
	dojo.xhrPost({
		url:url_group,	
		content:{ 
			'group_id': group
