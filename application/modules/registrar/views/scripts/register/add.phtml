	function getTotale(){
		tuitionfee=dijit.byId('tuitionfee').get('value');
		tuitionfee=isNaN(tuitionfee)?0:tuitionfee;
		late_fee = dijit.byId('late_fee').get('value');
		late_fee = isNaN(late_fee)?0:late_fee;
		discount=dijit.byId('discount').get('value');
		pc=discount/100;
		ds = (tuitionfee - late_fee) * pc;
		total_payment = (tuitionfee - late_fee) - ds;
		if(discount<=100){
		   dijit.byId('total_payment').attr('value',total_payment);
		}else{ 
			alert('Can not discount more than 100%');
			dijit.byId('discount').attr('value','100');
		}
		netTotal();
	}
	function calculateTotal(index){
		price = dijit.byId('price_'+index).get('value');
		qty = dijit.byId('qty_'+index).get('value');
		qty=isNaN(qty)?1:qty;
		if(qty==1){dijit.byId('qty_'+index).attr('value',qty);}
		subtotal = price * qty;
		late_fee_service = dijit.byId('late_fee_service_'+index).get('value');
		late_fee_service = isNaN(late_fee_service)?0:late_fee_service;
		additional_fee = dijit.byId('additional_fee_'+index).get('value');
		additional_fee = isNaN(additional_fee)?0:additional_fee;
		discount = dijit.byId('discount_'+index).get('value');
		discount = isNaN(discount)?0:discount;
		total = subtotal - late_fee_service + additional_fee;
		total_pay = total - (total*discount/100);
		dijit.byId('subtotal_'+index).attr('value',subtotal);
		dijit.byId('paidamount_'+index).attr('value',total_pay);
		netTotal();
	}
	function netTotal() {
		var netTotal=0;
		var netPaidAmount=0;
		var rowId = $('#identity').val();
		var rowIDArray = rowId.split(',');
		total_payment = dijit.byId('total_payment').get('value');
	/////////////////////////////////////////////////////////////////////	
		var payment_type = dijit.byId("payment_type").get("value");
		if(payment_type!=2){
			for(var n = 0; n < rowIDArray.length; n++) {
				netPaidAmount += dijit.byId('paidamount_'+rowIDArray[n]).get('value');
			}
		}		
		if(payment_type==1){
			grandTotal = parseFloat(netPaidAmount) + parseFloat(total_payment);
		}else if(payment_type==2){
			grandTotal = parseFloat(total_payment);
		}else if(payment_type==3){
			grandTotal = netPaidAmount;
		}		
		ex_rate = dijit.byId('ex_rate').get('value');
		credit_memo = dijit.byId('credit_memo').get('value');
		credit_memo = isNaN(credit_memo)?0:credit_memo;
		deduct = dijit.byId('deduct').get('value');
		deduct = isNaN(deduct)?0:deduct;
		netAmount = grandTotal - credit_memo - deduct;
		dijit.byId('grand_total').attr('value',grandTotal );
		dijit.byId('net_amount').attr('value',netAmount.toFixed(2));
		readMoneyInKhmer(netAmount.toFixed(2));
	}
