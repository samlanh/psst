 var url_getservicetype = '<?php echo $this->url(array('module'=>'registrar','controller'=>'register','action'=>'get-service-type')); ?>';
 function getServiceType(index){
	 service_id = dijit.byId('service_'+index).get('value');
	 //alert(service_id);
	 dojo.xhrPost({
			url:url_getservicetype,
			content:{
				'service_id':service_id,
				},
			handleAs:"json",
			load: function(data) {
			    	dijit.byId('service_type_'+index).attr('value',data);
			    	if(data==2){
			    		removeAddRowReadOnly(index);
			    		getservicefee(index);
			    		
			    	}else{
						getProductFee(index);
						setAddRowReadOnly(index);
			    	}
			},
			error: function(err) {
				//alert(err);
			}
		});
 }
	// Set readonly when service type=1 (product)	
 function setAddRowReadOnly(index){
		dijit.byId('term_'+index).attr('value','');
		dijit.byId('date_start_'+index).attr('value',null);
		dijit.byId('validate_'+index).attr('value',null);
		dijit.byId("date_start_"+index).set('readOnly',true);
		dijit.byId("validate_"+index).set('readOnly',true);
		dijit.byId("term_"+index).set('readOnly',true);
	}
// Remove field readonly when balance <= 0 for get all old information of student
function removeAddRowReadOnly(index){
		term = dijit.byId('term_'+index).get('value');
		if(term==''){
			dijit.byId('term_'+index).attr('value',1);
		}else{
			dijit.byId('term_'+index).attr('value',term);
		}
		dijit.byId('date_start_'+index).attr('value','<?php echo date('Y-m-d')?>');
		dijit.byId("date_start_"+index).set('readOnly',false);
		dijit.byId("validate_"+index).set('readOnly',false);
		dijit.byId("term_"+index).set('readOnly',false);
	}
