	//set value totale 
	function getTotale(){
		tuitionfee=dijit.byId('tuitionfee').get('value');
		tuitionfee=isNaN(tuitionfee)?0:tuitionfee;
		discount=dijit.byId('discount').get('value');
		pc=discount/100;
		ds=tuitionfee*pc;
		old_totale=tuitionfee - ds;
		dijit.byId('total_payment').attr('value',old_totale);
		dijit.byId('paid_amount').attr('value',old_totale);
		//calculateBalance();
		netTotal();
	}

	function calculateBalance(){
		total_payment = dijit.byId('total_payment').get('value');
		paid_amount = dijit.byId('paid_amount').get('value');

		//alert(total_payment);
		//alert(paid_amount);
		if(total_payment>=paid_amount){
			balance = total_payment - paid_amount;
			dijit.byId('balance').attr('value',balance);
		}else if(total_payment<paid_amount){
			alert("You can't pay more than total payment");
			dijit.byId('paid_amount').attr('value',total_payment);
			dijit.byId('balance').attr('value',0);
		}
	}
	
	function getDisccount(){
