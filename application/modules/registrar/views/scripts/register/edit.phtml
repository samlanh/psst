 function calculateTotal(index){
	 	price = dijit.byId('price_'+index).get('value');
		qty = dijit.byId('qty_'+index).get('value');
		qty=isNaN(qty)?1:qty;
		if(qty==1){dijit.byId('qty_'+index).attr('value',qty);}
		total = price*qty;
		discount = dijit.byId('discount_'+index).get('value');
		discount = isNaN(discount)?0:discount;
		dijit.byId('subtotal_'+index).attr('value',total-(total*(discount/100)));
		dijit.byId('paidamount_'+index).attr('value',total-(total*(discount/100)));
		netTotal();
 }

 function netTotal() {
	 //alert(1);
		var netTotal=0;
		var netPaidAmount=0;
		var rowId = $('#identity').val();
		//rowId = isNaN(rowId)?0:rowId;

		var rowIDArray = rowId.split(',');
		if(rowIDArray==''){
			rowIDArray = 1;
		}
		
		paid_amount = dijit.byId('paid_amount').get('value');
		total_payment = dijit.byId('total_payment').get('value');
		
		//////////////////// calculate admin fee ///////////////////////	
		fee = dijit.byId('tuitionfee').get('value');
		admin_fee = fee * 0.01;
		balance_ecord = dijit.byId('id_balance_record').get('value');
		if(balance_ecord!=''){
			admin_fee=0;
		}
		//admin_fee = dijit.byId('admin_fee').get('value');
		
	/////////////////////////////////////////////////////////////////////	
		
		var payment_type = $('input[name=payment_type]:checked').val();
		if(payment_type!=2){
			for(var n = 0; n < rowIDArray.length; n++) {
				subtotoal = dijit.byId('subtotal_'+rowIDArray[n]).get('value');
				//subtotoal=isNaN(subtotoal)?0:subtotoal;
				if(subtotoal==''){
					subtotoal = 0;
				}
				netTotal += subtotoal;

				paidamount = dijit.byId('paidamount_'+rowIDArray[n]).get('value');
				//paidamount = isNaN(paidamount)?0:paidamount;
				if(paidamount==''){
					paidamount = 0;
				}
				netPaidAmount += paidamount ;
			}
		}
		
		//alert(payment_type);
		
		if(payment_type==1){
			dijit.byId('admin_fee').attr('value',admin_fee);
			grandTotal = parseFloat(netTotal) + parseFloat(total_payment) + parseFloat(admin_fee);
			total_received = parseFloat(netPaidAmount) + parseFloat(paid_amount) + parseFloat(admin_fee);
		}else if(payment_type==2){
			dijit.byId('admin_fee').attr('value',admin_fee);
			grandTotal = parseFloat(total_payment) + parseFloat(admin_fee);
			total_received = parseFloat(paid_amount)+ parseFloat(admin_fee);
		}else if(payment_type==3){
			dijit.byId('admin_fee').attr('value',0);
			grandTotal = netTotal;
			total_received = netPaidAmount;
		}
		
		//grandTotal = (+netTotal) + (+tuitionfee);
		//total_received = (+netPaidAmount) + (+tuitionfee);
		
		dijit.byId('grand_total').attr('value',grandTotal);
		dijit.byId('convert_to_riels').attr('value',grandTotal * 4000);
		dijit.byId('total_received').attr('value',total_received);
		calculatBalanceAndReturn();
 }

