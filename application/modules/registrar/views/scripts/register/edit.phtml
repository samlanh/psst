	//set value totale 
	function getTotale(){
		tuitionfee=dijit.byId('tuitionfee').get('value');
		tuitionfee=isNaN(tuitionfee)?0:tuitionfee;
		discount=dijit.byId('discount').get('value');
		pc=discount/100;
		ds=tuitionfee*pc;
		old_totale=tuitionfee - ds;
		dijit.byId('total_payment').attr('value',old_totale);
		//dijit.byId('paid_amount').attr('value',old_totale);
		//calculateBalance();
		netTotal();
	}

	function calculateBalance(){
		total_payment = dijit.byId('total_payment').get('value');
		paid_amount = dijit.byId('paid_amount').get('value');

		//alert(total_payment);
		//alert(paid_amount);
		if(total_payment>=paid_amount){
			balance = total_payment - paid_amount;
			dijit.byId('balance').attr('value',balance);
		}else if(total_payment<paid_amount){
			alert("You can't pay more than total payment");
			dijit.byId('paid_amount').attr('value',total_payment);
			dijit.byId('balance').attr('value',0);
		}
	}
	
	function getDisccount(){
		tuitionfee=dijit.byId('tuitionfee').get('value');
		tuitionfee=isNaN(tuitionfee)?0:tuitionfee;
		discount=dijit.byId('discount').get('value');
		pc=discount/100;
		ds=tuitionfee*pc;
		totale=tuitionfee - ds;
		if(discount<=100){
			   dijit.byId('total_payment').attr('value',totale.toFixed(2));
			}else{ 
				alert('No more than a hundred of disccount!');
				dijit.byId('discount').attr('value','100');
			}
	}
	
//calculator date by select term_payment
	function getDateTerm() {
		payment_term=dijit.byId('payment_term').get('value');
		start_date=dijit.byId('start_date').get('value');
			var a = new Date(start_date);
			if(payment_term==1){
				var mm = a.getMonth() +1+ 1;
			}
			if(payment_term==2){
				var mm = a.getMonth() +3+ 1;
			}else if(payment_term==3){
				mm = a.getMonth()+6+1;
			}else if(payment_term==4){
				mm = a.getMonth()+12+1;
			}
				var dd = a.getDate();
				 if(dd<10){
					 dd = "0"+dd;
				}
				 if(mm<10){
					 mm = "0"+mm;
				}
				var y = a.getFullYear();
				var epx_date = y + '-'+ mm + '-'+dd ;
				dijit.byId('end_date').attr('value',epx_date);	
	}
	
