var url_qty = '<?php echo $this->url(array('module'=>'stock','controller'=>'requestproduct','action'=>'get-productqty')); ?>';
function getQtyById(index){
	pro_id = dijit.byId('pro_id').get('value');
	branch_id=dijit.byId("branch").get("value");
	if(pro_id==0 || pro_id=='' || branch_id==0 || branch_id==''){
		dijit.byId('curr_qty'+index).attr("value",0);
		return  false;
	}
		dojo.xhrPost({
			url:url_qty,
			content:{
				'pro_id':pro_id,
				'branch_id':branch_id
				},
			handleAs:"json",
			load: function(data) {
				if(data==0 || data==''){
					dijit.byId('pro_id').attr('value','');
					alert('Empty item qty!');
					dijit.byId('request_qty'+index).attr("value",''); 
				}else{
					dijit.byId('curr_qty'+index).attr("value",data.pro_qty);
					dijit.byId('cost_'+index).attr("value",data.cost);	
				}
			},
			error: function(err) {
				alert(err);
			}
		});
}
